{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.nuxt": true,
    "**/.output": true,
    "yarn.lock": true,
    "package-lock.json": true
  },
  "editor.formatOnSave": false,
  "vetur.validation.template": true,
  "vue3snippets.enable-compile-vue-file-on-did-save-code": false
}
